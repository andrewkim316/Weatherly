{
    "manifest_version": 2,
    "name": "Weatherly",
    "description": "Accurate, hourly weather, cross-checked with multiple sources.",
    "version": "1",
    "author": "Andrew Kim",
    "permissions": ["activeTab"],
    "browser_action": {
        "default_popup": "weatherly.html"
    },
    "content_security_policy": "script-src 'self' https://maps.googleapis.com; object-src 'self'"
}